[alias]
  ci = commit
  s  = status
  st = status
  a  = add
  d  = diff
  co = checkout
  b  = branch
  l  = log
  lp = log --decorate --pretty=oneline --abbrev-commit
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  ls = ls-files
  sh = show
  pl = pull
  plr = pull --rebase
  ps = push
  ulc= reset --soft HEAD^
  r  = reset
[user]
  name = Timo Rößner
  email = timo.roessner@googlemail.com
[push]
  default = tracking
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol

